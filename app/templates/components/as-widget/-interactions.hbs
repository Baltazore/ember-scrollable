{{#if interactions}}
  <ul>
    {{#each interaction in visibleContent}}
      <li>
        {{partial listItemTemplateName}}
      </li>
    {{/each}}
  </ul>

  {{#if hasMoreItems}}
    <div class="toggle-collapse" title={{collapseTitle}} {{action "toggleCollapse"}}></div>
  {{/if}}
{{else}}
  <div class="empty-message">{{emptyMessage}}</div>
{{/if}}
