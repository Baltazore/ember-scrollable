<div class="priority">
  {{as-priority-select project=model}}
</div>

<div class="details">
  <h1>
    <span>{{name}}</span>
    <small>{{clientCode}}</small>
  </h1>
</div>

<div class="members">
  {{as-avatar user=lead class="lead"}}

  {{#each members}}
    {{as-avatar user=this}}
  {{/each}}
</div>

{{as-project-progress
  hasTarget=hasDeliveryTarget
  deliveredCount=deliveredAdvisorsCount
  targetCount=deliveryTarget}}
