<div class="priority">
  {{as-priority-select project=model}}
</div>

<div class="details">
  <h1>
    <span>{{name}}</span>
    <small>{{clientCode}}</small>
  </h1>
</div>

<div class="members">
  {{as-avatar user=lead class="lead"}}

  {{#each members}}
    {{as-avatar user=this}}
  {{/each}}
</div>

<div {{bind-attr class=":progress hasDeliveryTarget::no-target"}}>
  {{as-project-progress deliveredCount=deliveredAdvisorsCount targetCount=deliveryTarget}}
</div>
